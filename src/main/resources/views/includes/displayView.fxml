<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="org.bessonov.calculator.controllers.DisplayController"
      alignment="CENTER_RIGHT"
      prefHeight="100.0"
      prefWidth="201.0"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets right="10.0"/>
   </padding>
   <Label fx:id="subDisplayLabel" text="subDisplay">
      <padding>
         <Insets top="10.0"/>
      </padding>
   </Label>
   <Label fx:id="displayLabel" text="Display">
      <padding>
         <Insets bottom="10.0"/>
      </padding>
   </Label>
</VBox>
